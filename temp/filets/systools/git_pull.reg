Windows Registry Editor Version 5.00
[HKEY_CLASSES_ROOT\Directory\shell\GitPull]
@="git pull"
[HKEY_CLASSES_ROOT\Directory\shell\GitPull\Command]
@="cmd.exe /c cd %1 && git pull && pause"

[HKEY_CLASSES_ROOT\Directory\Background\shell\GitPull]
@="git pull"
[HKEY_CLASSES_ROOT\Directory\Background\shell\GitPull\Command]
@="cmd.exe /c cd %v. && git pull && pause"
